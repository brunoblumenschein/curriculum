{
    "extends": "../tslint.json",
    "rules": {
        "max-line-length": [
            true,
            {
                "limit": 160
            }
        ],
        "directive-selector": [
            true,
            "attribute",
            "blumen",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "blumen",
            "kebab-case"
        ]
    }
}